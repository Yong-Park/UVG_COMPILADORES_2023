.data
	GP: .space 0
	LP: .space 0
.text
Main.a:
	li $t0, 100
	sw $t0, 0($s0)
	jr $ra
.text
Main.b:
	li $t0, 515
	sw $t0, 4($s0)
	jr $ra
.text
main:
	la $s0, GP
	la $s1, LP
	jal Main.a
	jal Main.b
	lw $t0, 0($s0)
	lw $t1, 4($s0)
	add $t0, $t0, $t1
	sw $t0, 4($s1)
	li $v0, 1
	move $a0, $t0
	syscall
	li $v0, 10
	syscall
